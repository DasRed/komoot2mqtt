K2M_KOMOOT_EMAIL=here_comes_the_email
K2M_KOMOOT_PASSWORD=here_comes_the_password

K2M_MQTT_HOST=mqtt://test.mosquitto.org
K2M_MQTT_USER=here_comes_the_user
K2M_MQTT_PASSWORD=here_comes_the_password

K2M_HOMEASSISTANT_DISCOVERY=true
K2M_HOMEASSISTANT_BASE_TOPIC=homeassistant

# optional... variables are possible like {sport}
K2M_HOMEASSISTANT_STATISTIC_ID=komoot-statistic
# optional... variables are possible like {sport}
K2M_HOMEASSISTANT_LAST_TOUR_ID=komoot-last-tour

#optional
