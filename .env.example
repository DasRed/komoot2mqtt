# K2M_HOMEASSISTANT_DATABASE_PASSWORD=here_comes_the_password
#optional
# K2M_HOMEASSISTANT_DATABASE_NAME=homeassistant
#optional
# K2M_HOMEASSISTANT_DATABASE_CHARSET=utf8mb4
#optional
# K2M_HOMEASSISTANT_ENTITY_TEMPLATE=sensor.komoot2mqtt_{device}_{type}_{key}

